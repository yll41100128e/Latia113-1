= '易'  # 可以調整這個參數\n","filtered_data = df[df['mission_difficulty'] == difficulty]\n","display(filtered_data)\n","\n","# 計算每個 mission_field 的平均正確率和平均答題時間\n","groupedEZ = filtered_data.groupby('mission_field').agg(\n","    avg_correct_rate=('correct_rate', 'mean'),\n","    avg_answer_duration=('answer_duration_sec', 'mean')\n",").reset_index()\n","\n","print(groupedEZ)\n","\n","\n","\n","\n","# 畫圖 - 條形圖比較不同 mission_field 的平均正確率與答題時間\n","fig, axes = plt.subplots(1, 2, figsize=(14, 6))\n","\n","# 平均正確率\n","sns.barplot(x='mission_field', y='avg_correct_rate', data=groupedEZ, ax=axes[0])\n","axes[0].set_title(f\"平均正確率 ({difficulty} 階段)\")\n","axes[0].set_ylabel(\"平均正確率\")\n","axes[0].set_xlabel(\"任務領域\")\n","\n","# 平均答題時間\n","sns.barplot(x='mission_field', y='avg_answer_duration', data=groupedEZ, ax=axes[1])\n","axes[1].set_title(f\"平均答題時間 ({difficulty} 階段)\")\n","axes[1].set_ylabel(\"平均答題時間 (秒)\")\n","axes[1].set_xlabel(\"任務領域\")\n","\n","plt.tight_layout()\n","plt.show()"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":1000},"id":"K1RltJ2MLgyw","executionInfo":{"status":"ok","timestamp":1733635434381,"user_tz":-480,"elapsed":10584,"user":{"displayName":"嘉燿呂","userId":"03478228728049620049"}},"outputId":"9deb7a0d-3450-4a8d-ebc4-3fad20b224a6"},"execution_count":18,"outputs":[{"output_type":"display_data","data":{"text/plain":["        mission_difficulty mission_field  correct_rate  answer_duration_sec\n","10                       易          生活知識           0.0                 10.0\n","11                       中            議題           0.4                 31.0\n","40                       難            議題           0.2                 15.0\n","64                       難          生活知識           0.2                  6.0\n","65                       難          生活知識           0.4                 11.0\n","...                    ...           ...           ...                  ...\n","1561510                  中            議題           0.0                  8.0\n"